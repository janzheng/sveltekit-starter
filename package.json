{
  "name": "sveltekit-starter",
  "version": "0.1.0",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "notes": "yarn install necessary to include any yarn linked components",
    "dev": "svelte-kit dev",
    "devopen": "svelte-kit dev --open",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "commit": "git add . -A ; git commit ; git push origin main ",
    "commitprod": "git add . -A ; git commit ; git push origin prod ",
    "prod": "link vercel directly to prod branch; don't push to prod",
    "surge": "yarn build; cd build; surge . -d sveltekit-starterama.surge.sh",
    "stage": "yarn install; vercel",
    "grabby": "node ./node_modules/@yawnxyz/grabby/grabby.js ../../../grabby.json ./src/lib/data/grabbydata.json",
    "plasmid": "ln -s ../../../plasmid/"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.9.2",
    "@markdoc/markdoc": "^0.1.3",
    "@notionhq/client": "^1.0.4",
    "@supabase/supabase-js": "^1.30.0",
    "@sveltejs/adapter-auto": "^1.0.0-next.17",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/adapter-vercel": "^1.0.0-next.59",
    "@sveltejs/kit": "^1.0.0-next.355",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@yawnxyz/grabby": "^0.0.2",
    "@yawnxyz/sveltekit-notion": "^0.0.395",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.15",
    "csvtojson": "^2.0.10",
    "cytosis": "^0.0.5",
    "deta": "^1.1.0",
    "dotenv": "^15.0.0",
    "gridjs": "^5.0.2",
    "gridjs-svelte": "^2.1.0",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.15",
    "mdsvex": "^0.9.8",
    "node-cache": "^5.1.2",
    "notion-to-md": "^2.3.3",
    "plasmid": "git+https://github.com/janzheng/plasmid.git",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-nesting": "^10.1.1",
    "postcss-scss": "^4.0.2",
    "rehype-slug": "^5.0.1",
    "remark-attr": "^0.11.1",
    "sass": "^1.45.2",
    "svelte": "^3.48.0",
    "svelte-markdoc": "^0.0.1",
    "svelte-preprocess": "^4.10.1",
    "svelte-seo": "^1.3.1",
    "sveltejs-adapter-ipfs": "^0.2.1",
    "tailwindcss": "^3.0.7",
    "tw-elements": "^1.0.0-alpha10",
    "typescript": "^4.7.4"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/inter": "^4.5.1",
    "@lukeed/uuid": "^2.0.0",
